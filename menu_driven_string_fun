while True:
  print("swati")
  print("Menu Driven Program")
  print("1.Returns True if the string follows the rules of a title") #istitle()
  print("2.Returns True if all characters in the string are upper case") #isupper()
  print("3.Returns True if all characters in the string are whitespaces") #isspace()
  print("4.Converts the elements of an iterable into a string") #join()
  print("5.Returns a string where a specified value is replaced with a specified value") #replace()
  print("6.Swaps cases, lower case becomes upper case and vice versa") #swapcase()
  print("7.Splits the string at the specified separator, and returns a list") #split()
  print("8.Converts the first character of each word to upper case") #title()
  print("9.Converts a string into upper case") #upper()
  print("10.Return length of a string") #len()
  print("11.Exit")
  choice=int(input("Enter your choice:"))
  if choice==1:
    str="Swati"
    print(str.istitle())
  elif choice==2:
    str="swati"
    print(str.isupper())
  elif choice==3:
    str="swati"
    print(str.isspace())
  elif choice==4:
    str="swati"
    print("#".join(str))
  elif choice==5:
    str="sweti"
    print(str.replace("e","a"))
  elif choice==6:
    str="sWATi"
    print(str.swapcase())
  elif choice==7:
    str="swati"
    print(str.split("$"))
  elif choice==8:
    str="swati"
    print(str.title())
  elif choice==9:
    str="swati"
    print(str.upper())
  elif choice==10:
    str="swati"
    print(len(str))
  elif choice==11:
    break
  else:
    print("wrong output")
